{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <!--scope="col"은  th요소가  열  전체에  대해  헤더역할을  한다는  뜻-->
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    <!-- article 객체 -->
    {{#article}}
        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </tr>
    {{/article}}
    </tbody>
</table>
<button type="button"><a href="/articles/{{article.id}}/edit" class="btn btn-primary">수정</a></button>
<button type="button"><a herf="/articles/{{article.id}}/delete" class="btn btn-primary">삭제</a></button>
<button type="button"><a href="/articles" class="btn btn-primary">목록 페이지</a></button>
{{>layouts/footer}}